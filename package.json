{
	"name": "vslab",
	"displayName": "vslab",
	"description": "For own project",
	"publisher": "sqz",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vslab.version",
				"title": "VSLab Version"
			}
		],
		"configuration": {
			"type": "object",
			"title": "VSLab Configuration",
			"properties": {
				"vslab.localization.formatOnSave": {
					"type": "boolean",
					"default": false,
					"description": "Auto format on save"
				},
				"vslab.localization.exportCSOnSave": {
					"type": "boolean",
					"default": true,
					"description": "Auto export c# on save"
				}
			}
		},
		"keybindings": [
			{
				"key": "alt+e",
				"command": "cursorEnd",
				"when": "textInputFocus"
			},
			{
				"key": "alt+h",
				"command": "cursorHome",
				"when": "textInputFocus"
			}
		]
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/js-yaml": "^4.0.0",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"js-yaml": "4.0.0",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}
